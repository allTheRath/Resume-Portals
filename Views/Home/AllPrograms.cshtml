@model IEnumerable<Resume_Portal.Models.Program>


<div class="main-box">

    @foreach (var item in Model)
    {
        <div class="flex-data">
            <div class="flex-box">
                <img src="~/program-images/@item.Name/pic/logo.jpg" />
            </div>
            <div class="flex-box">
                <div>
                    @Html.DisplayFor(modelItem => item.Name)
                </div>
                <div>
                    @Html.DisplayFor(modelItem => item.Discription)
                </div>
                <div>
                    @Html.DisplayFor(modelItem => item.Duration)
                </div>
                <div>
                    @Html.ActionLink("Program Details", "ProgramDetails", new { id = item.Id })
                </div>
                <div>
                    @Html.ActionLink("All Students", "StudentInProgram", new { id = item.Id })
                </div>
            </div>

        </div>
    }

</div>
