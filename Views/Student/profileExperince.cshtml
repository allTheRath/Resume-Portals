@model IEnumerable<Resume_Portal.Models.Experiance>

@{
    Layout = null;
}


<h3>
    @Html.ActionLink("Add Experience", "AddExperience")
</h3>
    <table class="table">
        <tr>
            <th>
                @Html.DisplayName("InstituteName")
            </th>
            <th>
                @Html.DisplayName("Discription")
            </th>
            <th>
                @Html.DisplayName("Start")
            </th>
            <th>
                @Html.DisplayName("End")
            </th>
            <th></th>
        </tr>
    
    @foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.InstituteName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Discription)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Start)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.End)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
                @Html.ActionLink("Details", "Details", new { id=item.Id }) |
                @Html.ActionLink("Delete", "Delete", new { id=item.Id })
            </td>
        </tr>
    }
    
</table>

