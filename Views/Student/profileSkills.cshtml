@model Resume_Portal.Models.CompleteStudentInfo


<p>
    @Html.ActionLink("Add Skill", "AddSkill")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayName("Skill Name")
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model.Skills)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.SkillName)
            </td>
            <td>
                @Html.ActionLink("Delete", "DeleteSkill", new { id = item.Id })
            </td>
        </tr>
    }

</table>